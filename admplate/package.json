{
    "name": "admplate",
    "version": "5.0.0",
    "private": true,
    "description": "Admin template",
    "main": "server.js",
    "scripts": {
        "clean:dist": "del-cli static/dist/** && del-cli lib/**",
        "build": "npm run clean:dist && webpack --config webpack.config.production.js",
        "dev": "npm run clean:dist && webpack && babel src -d lib",
        "dev:watch": "npm run clean:dist && webpack --watch",
        "start": "nodemon --watch src --watch app server",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "config": {
        "css": "[name]_[local]__[hash:base64:5]"
    },
    "keywords": [
        "css-modules"
    ],
    "author": "jBox",
    "dependencies": {
        "@babel/register": "^7.4.4",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.19.0",
        "chaos-fetch": "^5.0.0",
        "chaos-magician": "^5.0.0",
        "classnames": "^2.2.6",
        "config-vars": "^5.0.0",
        "css-modules-require-hook": "^4.2.3",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "jquery": "^3.4.1",
        "jwt-decode": "^2.2.0",
        "lodash": "^4.17.11",
        "morgan": "^1.9.1",
        "prop-types": "^15.7.2",
        "qs": "^6.7.0",
        "react": "^16.8.6",
        "react-autocomplete": "^1.8.1",
        "react-browser-router": "^5.0.0",
        "react-dom": "^16.8.6",
        "react-dropzone": "^10.1.5",
        "react-infinite-scroller": "^1.2.4",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-select2-wrapper": "^1.0.4-beta6",
        "react-switch": "^5.0.0",
        "react-viewer": "^2.9.1",
