{
    "name": "admplate",
    "version": "5.0.0",
    "private": true,
    "description": "Admin template",
    "main": "lib/index.js",
    "scripts": {
        "prepublish": "npm run build",
        "clean:dist": "del-cli --force dist/**",
        "clean:lib": "del-cli --force lib/**",
        "clean": "npm run clean:dist && npm run clean:lib",
        "build": "npm run clean && webpack --config webpack.config.production.js && babel src -d lib",
        "dev:dist": "npm run clean:dist && webpack --watch",
        "dev:lib": "npm run clean:lib &&babel src -d lib --watch",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "jBox",
    "dependencies": {
        "chaos-fetch": "5.0.0",
        "chaos-magician": "5.0.0",
        "classnames": "2.2.6",
        "jquery": "3.4.1",
        "jwt-decode": "2.2.0",
        "lodash": "4.17.11",
        "prop-types": "15.7.2",
        "qs": "6.7.0",
        "react-autocomplete": "1.8.1",
        "react-router-ads": "5.0.0",
        "react-dropzone": "10.1.5",
        "react-infinite-scroller": "1.2.4",
        "react-select2-wrapper": "1.0.4-beta6",
        "react-switch": "5.0.0",
        "react-viewer": "2.9.1",
        "redux-logger": "3.0.6",
        "redux-thunk": "2.3.0",
        "reselect": "4.0.0",
        "uuid": "3.3.2"
    },
    "devDependencies": {
        "@babel/cli": "7.4.4",
