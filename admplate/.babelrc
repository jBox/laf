{
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "css-modules-transform",
            {
                "mode": "local",
                "generateScopedName": "[name]_[local]__[hash:base64:5]"
            }
        ]
    ],
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ]
}